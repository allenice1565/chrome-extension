{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "ESNext",
        "target": "es5",
        "lib": ["ESNext", "DOM"],
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "paths": {
            "@/*": ["./src/*"],
            "@pages*": ["./src/pages/*"],
            "@store/*": ["./src/store/*"],
            "@utils/*": ["./src/utils/*"]
        }
    }
}
